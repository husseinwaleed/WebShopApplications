@model webshoproject.Models.shoppingcart

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>


<table class="table">
   
  @foreach (var item in Model.car)
  {
     <tr>
        <td>
            @Html.DisplayFor(modelItem => item.model)
        </td>
        <td>
            @Html.DisplayFor(model => item.factory)
        </td>
    </tr>


    <tr>

        <td>
          
            @Html.ActionLink("Details", "Views/Cars/Details", new { id = item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.Id })
        </td>
    </tr>


</table>


  }


